                app:layout_constraintTop_toTopOf="parent"/>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="490dp"
                android:background="@null"
                android:gravity="center_vertical"
                android:text="관심있는 학교"
                android:paddingLeft="44dp"
                android:fontFamily="@font/font"
                android:textColor="@color/blackTitle"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <EditText
                android:id="@+id/edit_text3"
                android:layout_width="320dp"
                android:layout_height="49dp"
                android:layout_marginTop="530dp"
                android:background="@drawable/round_rect"
                android:inputType="text"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="585dp"
                android:background="@null"
                android:gravity="center_vertical"
                android:text="관심있는 전공 및 학과"
                android:paddingLeft="44dp"
                android:fontFamily="@font/font"
                android:textColor="@color/blackTitle"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <EditText
                android:id="@+id/edit_text4"
                android:layout_width="320dp"
                android:layout_height="49dp"
                android:layout_marginTop="625dp"
                android:background="@drawable/round_rect"
                android:inputType="text"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="680dp"
                android:background="@null"
                android:gravity="center_vertical"
                android:text="거주지"
                android:paddingLeft="44dp"
                android:fontFamily="@font/font"
                android:textColor="@color/blackTitle"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <EditText
                android:id="@+id/edit_text5"
                android:layout_width="320dp"
                android:layout_height="49dp"
                android:layout_marginTop="720dp"
                android:background="@drawable/round_rect"
                android:inputType="text"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="775dp"
                android:background="@null"
                android:gravity="center_vertical"
                android:text="이메일"
                android:paddingLeft="44dp"
                android:fontFamily="@font/font"
                android:textColor="@color/blackTitle"
                android:textSize="17sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <EditText
                android:id="@+id/edit_text6"
                android:layout_width="320dp"
                android:layout_height="49dp"
                android:layout_marginTop="815dp"
                android:background="@drawable/round_rect"
                android:inputType="text"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="870dp"
                android:background="@null"
                android:gravity="center_vertical"
                android:text="생년월일"
                android:paddingLeft="44dp"
                android:fontFamily="@font/font"
                android:textColor="@color/blackTitle"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
